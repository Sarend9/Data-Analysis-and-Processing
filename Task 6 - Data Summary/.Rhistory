#  оформление
ylim = c(0, 0.025),
col = "light blue",
main = "Диаграмма распределения исходных данных",
xlab = "результат, сырые баллы",
ylab =  "доля"
)
x5.7.var <- sum((x5.7$trait - mean(x5.7$trait)) ^ 2)/length(x5.7$trait)
x5.7.sd  <- sqrt(x5.7.var)
